{
  "webdevice": {
    "defaultDevice": "Direct",
    "baseUrl": "https://webdevice.io",
    "devices": {
      "Direct": {
        "aliases": [
          "Local Direct"
        ],
        "driver": "org.openqa.selenium.firefox.FirefoxDriver",
        "capabilities": {
          "version": "59"
        },
        "pooled": true
      },
      "Remote": {
        "aliases": [
          "iPhone"
        ],
        "remoteAddress": "http://selenium.grid:4444/wd/hub",
        "capabilities": {
          "version": "60",
          "username": "${saucelabs_username}",
          "accessKey": "${saucelabs_accessKey}"
        },
        "pooled": false,
        "confidential": [
          "accessKey"
        ]
      }
    }
  }
}